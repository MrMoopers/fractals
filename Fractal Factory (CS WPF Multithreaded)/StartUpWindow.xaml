<Window x:Class="FractalFactory.StartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StartUpWindow" Height="300" Width="249">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="269" VerticalAlignment="Top" Width="241">
            <Button x:Name="BtnDecimal" Content="Decimal" Width="100" Height="30" Margin="10,60,10,10" MouseLeftButtonDown="BtnDecimal_MouseLeftButtonDown" />
            <Button x:Name="BtnDouble" Content="Double" Width="100" Height="30" Margin="10,10,10,10" MouseLeftButtonDown="BtnDouble_MouseLeftButtonDown" />
        </StackPanel>
    </Grid>
</Window>
